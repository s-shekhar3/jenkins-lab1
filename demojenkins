pipeline {
    agent any
    stages {
        stage('Checkout App Repo') {
            steps {
                // Checkout your source code from version control
                git 'https://github.com/hkshitesh/SL-MAVEN-24-JAN-2026.git'
                
            }
        }
        stage('Build') {
            steps {
                // Use Maven to build your project
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                // Run tests if applicable
                sh 'mvn test'
            }
        }       
    }
} 
